{
    "name": "czim/laravel-cms-media-library",
    "description": "Laravel CMS: Media Library",
    "keywords": [ "cms", "laravel", "module", "uploads", "media library" ],
    "homepage": "https://github.com/czim",
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "czim/laravel-cms-core": "~1.3",
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "3.3.*",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Czim\\CmsMediaLibrary\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\CmsMediaLibrary\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
